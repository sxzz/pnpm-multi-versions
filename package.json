{
  "name": "pnpm-multi-versions",
  "type": "module",
  "version": "0.6.0",
  "packageManager": "pnpm@10.28.2",
  "description": "Find multiple versions of dependencies from pnpm lockfile.",
  "author": "Kevin Deng <sxzz@sxzz.moe>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sxzz",
  "homepage": "https://github.com/sxzz/pnpm-multi-versions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/pnpm-multi-versions.git"
  },
  "bugs": {
    "url": "https://github.com/sxzz/pnpm-multi-versions/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./cli-run": "./dist/cli-run.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "bin": {
    "pnpm-multi-versions": "./dist/cli-run.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "node src/cli-run.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "empathic": "^2.0.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.6.0",
    "@sxzz/prettier-config": "^2.3.1",
    "@types/node": "^25.2.1",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "tsdown-preset-sxzz": "^0.3.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "prettier": "@sxzz/prettier-config"
}
